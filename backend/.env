# Environment
ENVIRONMENT=development
LOG_LEVEL=INFO

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000

# Audio Processing
SAMPLE_RATE=44100
N_MELS=128
N_FFT=2048

# MATLAB
MATLAB_PATH=matlab
RUN_MATLAB_ANALYSIS=true
MATLAB_TIMEOUT=60

# Caching
ENABLE_CACHING=true
CACHE_TTL=3600

# File Limits
MAX_FILE_SIZE=52428800

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8000,http://127.0.0.1:3000
